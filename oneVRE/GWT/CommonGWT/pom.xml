    <modelVersion>4.0.0</modelVersion>
    <artifactId>CommonGWT</artifactId>
    <packaging>jar</packaging>
    <name>GWT Common Classes for PAG/OneVRE</name>

    <build>
        <finalName>CommonGWT</finalName>
        <resources>
            <resource>
                <directory>src/main/java</directory>
                <includes>
                    <include>com/googlecode/onevre/gwt/**</include>
                </includes>
            </resource>
            <resource>
                <directory>src/main/resources</directory>
            </resource>
        </resources>
    </build>
